    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2015-07-10</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2015-07-23T16:42:52Z</lastSummaryUpdateDate>
          <text><![CDATA[<p>This bill requires that health care claims submitted with ICD-9 codes (the code set currently used in health care transactions to identify diagnoses and procedures) continue to be paid during the transition from the ICD-9 code set to the ICD-10 code set.</p>]]></text>
          <updateDate>2015-07-10T04:00:00Z</updateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
