    <summaries>
      <billSummaries>
        <item>
          <actionDate>2017-10-18</actionDate>
          <versionCode>00</versionCode>
          <name>Introduced in Senate</name>
          <updateDate>2017-10-18T04:00:00Z</updateDate>
